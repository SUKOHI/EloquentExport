{
    "name": "sukohi/eloquent-export",
    "description": "A Laravel package that allows you to export data from database through Eloquent model.",
    "authors": [
        {
            "name": "sukohi",
            "email": "sukohi.capilano@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.7|^6.0",
        "sukohi/fluent-csv": "3.*",
        "phpoffice/phpspreadsheet": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Sukohi\\EloquentExport\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Sukohi\\EloquentExport\\EloquentExportServiceProvider"
            ]
        }
    }
}
